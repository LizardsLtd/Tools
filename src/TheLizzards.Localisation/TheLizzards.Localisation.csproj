<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>The geocoidng service.</Description>
    <Copyright>TheLizzards 2016 - 2017</Copyright>
    <AssemblyTitle>TheLizzards.Localisation</AssemblyTitle>
    <VersionPrefix>1.4.2</VersionPrefix>
    <Version>1.4.2</Version>
    <AssemblyName>TheLizzards.Localisation</AssemblyName>
    <PackageTags>Localisation;thelizzards</PackageTags>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/TheLizzards/tools</RepositoryUrl>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
